{
  "name": "david",
  "description": "Nodejs module that tells you when your project npm dependencies are out of date.",
  "version": "2.2.0",
  "main": "lib/david.js",
  "scripts": {
    "test": "nodeunit test",
    "coveralls": "jscoverage lib && DAVID_COV=1 nodeunit --reporter=lcov test | coveralls"
  },
  "license": {
    "type": "MIT",
    "url": "https://github.com/alanshaw/david/raw/master/LICENSE"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/alanshaw/david.git"
  },
  "dependencies": {
    "npm": "~1.3.6",
    "semver": "~2.1.0",
    "optimist": "~0.6.0",
    "async": "~0.2.9"
  },
  "devDependencies": {
    "nodeunit": "~0.8.1",
    "jscoverage": "~0.3.7",
    "coveralls": "~2.3.0",
    "rewire": "~2.0.0",
    "rimraf": "~2.2.2"
  },
  "bin": {
    "david": "./bin/david.js"
  },
  "engines": {
    "node": "~0.10.1"
  }
}
